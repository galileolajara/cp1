         if (at.type == #struct-enum) {
            if (at.decl.enum == #nil) {
               'c.out.printf("%s:%u:%u: function using ':this' was declared on :%s which is not defined\n", f.file.path(), f.begin-row, f.begin-col, at.name.id.str());
               'c.exit(#failure);
            }
