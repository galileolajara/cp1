            var group-v'u8[1];
            group-v[0] = 1;
            var carg-v'expr-i[1];
            e-bool! = 'expr-i.alloc();
            'expr-i.set-bool(e-bool, true);
            carg-v[0] = e-bool;
            'expr-i.set-method(e-method, s.expr, f.decl.name, false, 1, group-v, 1, carg-v);
            s.expr = e-method;
            s.is-overload = true;
