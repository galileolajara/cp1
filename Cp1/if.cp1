      s.expr.write-value(s.val);
      output{") {\n"}
   }
   process-if(stmt'stmt, ok'bool&) @case.process() @inline {
      var s'stmt-if = stmt;
      if !s.expr.value(1, false, s.val) {
         return;
      }
      type-ptr! = s.val.type.ptr();
      if type-ptr.def == #enum {
         en! = type-ptr.decl.enumm.ptr();
